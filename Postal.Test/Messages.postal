namespace Postal.Test;

enum Result
{
	UnknownError = 0;
	Exception;
	CouldNotFindKey;
	Success;
}

message SetStrings
{
	request
	{
		mandatory string[] Names;
		mandatory string[] Values;
	}
	response
	{
		mandatory Result Result;
		string Message;
	}
}

message GetStrings
{
	request
	{
		mandatory string[] Names;
	}
	response
	{
		mandatory Result Result;
		string Message;
		mandatory string[] Values;
	}
}